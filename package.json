{
  "name": "directus-extension-sql-query-manager",
  "version": "1.0.0",
  "keywords": ["directus", "directus-extension", "directus-custom-bundle"],
  "directus:extension": {
    "type": "bundle",
    "path": {
      "api": "dist/api.js",
      "app": "dist/app.js"
    },
    "entries": [
      {
        "type": "endpoint",
        "name": "sql-query-manager-endpoint",
        "source": "src/index.js",
		"path": "dist/api.js",
		"hidden": false
      },
      {
        "type": "module",
        "name": "sql-query-manager-module",
        "source": "src/module/index.js",
		"path": "dist/api.js",
		"hidden": false
      }
    ],
    "host": "^10.0.0"
  },
  "scripts": {
    "build": "directus-extension build"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "^10.0.0"
  }
}